<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title><%= PROJECT_NAME.humanize %></title>
<!-- Style Sheets -->
<link rel="stylesheet" type="text/css" href="/css/reset.css"  />
<link rel="stylesheet" type="text/css" href="/css/structure.css"  />
<link rel="stylesheet" type="text/css" href="/css/defaults.css"  />
<link rel="stylesheet" type="text/css" href="/css/pingshots.css" title="Click to switch to the Brown Color Scheme" media="screen" />
<link rel="alternate stylesheet" type="text/css" href="/css/blue.css" title="Click to switch to the Blue Color Scheme" media="screen" />
<link rel="alternate stylesheet" type="text/css" href="/css/bright.css" title="Click Here to Activate the Bright Color Scheme" media="screen" />
<link rel="stylesheet" type="text/css" href="/css/jush.css"  />
<link rel="stylesheet" type="text/css" href="/css/datepicker.css"  />
<!--[if lt IE 8]>
<script src="http://ie7-js.googlecode.com/svn/version/2.0(beta3)/IE8.js"
type="text/javascript"></script>
<link rel="stylesheet" href="css/ie7.css" type="text/css" media="screen"/>
<![endif]-->
<!-- jQuery and Plugins - Do not need to edit them -->
<script type="text/javascript" src="/js/jquery.min.js"></script>
<!--Syntax Highlighter -->
<script type="text/javascript" src="/js/jquery.jush.js"></script>
<!-- Validate Forms -->
<script type="text/javascript" src="/js/jquery.validate.min.js"></script>
<!-- Style Switcher Forms -->
<script type="text/javascript" src="/js/styleswitch.js"></script>
<!--datepicker-->
<script type="text/javascript" src="/js/jquery.datepicker.js"></script>
<script type="text/javascript" src="/js/date.js"></script>
<%= javascript_include_tag :defaults %>
<!--[if IE]><script type="text/javascript" src="js/jquery.bgiframe.js"></script><![endif]-->
<!-- Custom Javascript - If you do need to make changes in the js, make them in the file below -->
<script language="javascript" type="text/javascript" src="/js/js.js"></script>
<!-- Favicon, to change, just replace the favicon file with yours. To make your own, I find favicon.cc a good site  -->
<link href="/favicon.ico" rel="shortcut icon" type="image/x-icon" />
<link rel="icon" href="/favicon.ico" />
</head>
<body>
<div id="header-logo">
  <h1>Admin Skin</h1>
</div>
<!-- End Span #header-logo-->
<div id="top-stats">
  <p>Welcome <a href="#">Admin</a> | <a href="#">Settings</a> | <a href="login.html">Log Out</a></p>
  <br />
  <p class="sub-brown">Last Logged in on Oct. 21<br />
    Last login ip: 74.132.0.13</p>
</div>
<!--End Span #top-stats -->
<div id="content-container">
  <div id="top-tabs"> 
	<span>
		<a href="/" <%= "class='active-tab'" if request.request_uri == ("/")%>>Dashboard</a>
	</span> 
	<span>
		<a href="/sites" <%= "class='active-tab'" if request.request_uri.include?("/sites")%>>Sites</a>
	</span> 
	<span><a href="#">Reports</a></span> 
	<span><a href="notifications.html">Contacts</a></span> 
	<span><a href="forms.html">Help</a></span> 
  </div>
  <!-- End Div #top-tabs -->
  <div id="content">
    <!-- <input type="text" name="search" value="Search..." class="small-form" id="search-form" /> -->
   <%= yield %>
	<div >
		<% if ENV["RAILS_ENV"] == "development" %>
		<h3>This message and following is only seen in development mode</h3>
			<div id="debug">
				<a href="*" onClick="Element.toggle('params_debug_info'); return false">params</a>
				<a href="*" onClick="Element.toggle('session_debug_info'); return false">sessions</a>
				<a href="*" onClick="Element.toggle('env_debug_info'); return false">env</a>
				<a href="*" onClick="Element.toggle('request_debug_info'); return false">request</a>
				<fieldset id ="params_debug_info" class="debug_info">
					<legend>params</legend>
					<%= debug(params) %>
				</fieldset>

				<fieldset id ="session_debug_info" class="debug_info" style="display:none">
					<legend>session</legend>
					<%= debug(session) %>
				</fieldset>

				<fieldset id ="env_debug_info" class="debug_info" style="display:none">
					<legend>env</legend>
					<%= debug(request.env) %>
				</fieldset>

				<fieldset id ="request_debug_info" class="debug_info" style="display:none">
					<legend>request</legend>
					<%= debug(request) %>
				</fieldset>


			</div>

		<% end %>
	<p>Click Here CMS version .02</p>
	</div>
  </div>
  <!-- End Div #content -->
</div>
<!-- End Div #content-container -->
<div id="side-menu-container">
